{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "global-require": 0,
        "consistent-return": 0,
        "max-len": [2, 125, 1]
    },
    "globals": {
        "Models": true,
        "Services": true,
        "Utils": true,
        "CreateErrReply": true,
        "_": true,
        "bignumberify": true
    }
}
